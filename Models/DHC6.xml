<?xml version="1.0"?>

<PropertyList>

<path>dhc-6.ac</path>

    <nasal>
        <load>
            var livery_update = aircraft.livery_update.new("Aircraft/dhc6/Models/Liveries", 10);
        </load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal>


    <animation>
        <name>Exterior</name>
      <object-name>fuselage</object-name>
      <object-name>fuselage.004</object-name>
      <object-name>longnose</object-name>
      <object-name>bluntnose</object-name>
      <object-name>Spinners</object-name>
      <object-name>RHdoor</object-name>
      <object-name>LHdoor</object-name>
      <object-name>Rrdoor.rear</object-name>
      <object-name>Rrdoor.frt</object-name>
      <object-name>Baggage.door</object-name>
      <object-name>wings</object-name>
      <object-name>wingbrace</object-name>
      <object-name>R.aileron</object-name>
      <object-name>R.flap</object-name>
      <object-name>L.flap</object-name>
      <object-name>R.slat</object-name>
      <object-name>L.slat</object-name>
      <object-name>L.aileron</object-name>
      <object-name>Hstab</object-name>
      <object-name>rudder</object-name>
      <object-name>Elevator</object-name>
      <object-name>Engine</object-name>
      <object-name>Engine.cowl</object-name>
    </animation>

    <animation>
        <type>material</type>
         <object-name>Exterior</object-name>
      <property-base>sim/model/livery</property-base>
      <texture-prop>texture</texture-prop>
        <texture>paint1.png</texture>
    </animation>

    <model>
        <name>cabin</name>
        <path>Aircraft/dhc6/Models/cabin.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>flightdeck</name>
        <path>Aircraft/dhc6/Models/flightdeck.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>wheels</name>
        <condition>
            <equals>
                <property>sim/multiplay/generic/string</property>
                <value>dhc6</value>
            </equals>
        </condition>
        <path>Aircraft/dhc6/Models/wheels.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>floats</name>
        <condition>
            <equals>
                <property>sim/multiplay/generic/string</property>
                <value>dhc6F</value>
            </equals>
        </condition>
        <path>Aircraft/dhc6/Models/floats.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <animation>
        <type>shader</type>
        <shader>chrome</shader>
        <texture>Aircraft/dhc6/Models/chrome1.png</texture>
        <object-name>glass</object-name>
        <object-name>LHdoor.glass</object-name>
        <object-name>RHdoor.glass</object-name>
        <object-name>Rrdoor.glass</object-name>
    </animation>

    <animation>
        <type>select</type>
        <object-name>bluntnose</object-name>
        <condition>
            <equals>
                <property>sim/multiplay/generic/string</property>
                <value>dhc6F</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>longnose</object-name>
        <condition>
            <not-equals>
                <property>sim/multiplay/generic/string</property>
                <value>dhc6F</value>
            </not-equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Rrdoor.frt.001</object-name>
        <object-name>Rrdoor.frt</object-name>
        <object-name>Rrdoor.rear.001</object-name>
        <object-name>Rrdoor.rear</object-name>
        <object-name>Rrdoor.glass</object-name>
        <condition>
            <not>
            <property>sim/model/remove-doors</property>
            </not>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>navlights</object-name>
        <condition>
            <not-equals>
            <property>systems/electrical/outputs/nav-lights</property>
            <value>0</value>
            </not-equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>beacon</object-name>
        <condition>
            <not-equals>
            <property>systems/electrical/outputs/beacon</property>
            <value>0</value>
            </not-equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>RHLNDlight</object-name>
        <object-name>LHLNDlight</object-name>
        <condition>
            <not-equals>
            <property>systems/electrical/outputs/landing-lights</property>
            <value>0</value>
            </not-equals>
        </condition>
    </animation>

    <animation>
        <type>flash</type>
        <object-name>RHLNDlight</object-name>
        <center>
            <x-m>-1.017</x-m>
            <y-m>3.744</y-m>
            <z-m>1.128</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <power>2</power>
        <factor>2</factor>
        <offset>0</offset>
        <two-sides type="bool">false</two-sides>
        <min>0.5</min>
        <max>2</max>
    </animation>

    <animation>
        <type>flash</type>
        <object-name>LHLNDlight</object-name>
        <center>
            <x-m>-1.017</x-m>
            <y-m>-3.744</y-m>
            <z-m>1.128</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <power>2</power>
        <factor>2</factor>
        <offset>0</offset>
        <two-sides type="bool">false</two-sides>
        <min>0.5</min>
        <max>2</max>
    </animation>
<!--    Wipers   -->

    <animation>
        <type>rotate</type>
        <object-name>RHwiper</object-name>
        <object-name>RHwiper.blade</object-name>
        <property>controls/electric/wipers/position-norm</property>
        <factor>90.0</factor>
        <center>
            <x-m>-4.122</x-m>
            <y-m>0.400</y-m>
            <z-m>0.5225</z-m>
        </center>
        <axis>
            <x>-0.688</x>
            <y>0.235</y>
            <z>0.687</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RHwiper.arm</object-name>
        <property>controls/electric/wipers/position-norm</property>
        <factor>90.0</factor>
        <center>
            <x-m>-4.140</x-m>
            <y-m>0.360</y-m>
            <z-m>0.540</z-m>
        </center>
        <axis>
            <x>-0.688</x>
            <y>0.235</y>
            <z>0.687</z>
